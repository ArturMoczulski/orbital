    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "commander": "^11.1.0",
    "jest": "^29.0.0",
    "nats": "^2.19.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "zod": "^3.22.4"
