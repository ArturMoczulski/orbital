{
  "name": "@orbital/react-ui",
  "version": "0.1.0",
  "description": "React UI component library",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./*": {
      "import": "./dist/src/*.js",
      "require": "./dist/src/*.js",
      "types": "./dist/src/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "cypress run --component",
    "test:e2e": "cypress run",
    "test:component": "cypress run --component",
    "test:all": "npm run test:component && npm run test:e2e && npm run test:unit",
    "test:open": "cypress open --component",
    "test:open:e2e": "cypress open",
    "test:unit": "NODE_NO_WARNINGS=1 jest --config jest.unit.config.js --passWithNoTests"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "notistack": "^3.0.2",
    "react-hook-form": "^7.58.1",
    "uniforms": "^4.0.0",
    "uniforms-antd": "^4.0.0",
    "uniforms-bridge-zod": "^4.0.0",
    "uniforms-mui": "^4.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.18",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "css-loader": "^6.7.3",
    "cypress": "^14.5.0",
    "cypress-terminal-report": "^7.2.0",
    "jest": "^30.0.0",
    "lodash": "^4.17.21",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/styled-engine": "^7.1.1",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "packageExtensions": {
    "@mui/styled-engine@*": {
      "dependencies": {
        "@emotion/react": "11.14.0",
        "@emotion/styled": "11.14.0"
      }
    }
  },
  "resolutions": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/material": "7.1.2",
    "@mui/system": "7.1.2",
    "@mui/icons-material": "7.1.2",
    "@mui/utils": "7.1.2"
  },
  "license": "UNLICENSED"
}
